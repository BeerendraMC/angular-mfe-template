{
  "name": "angular-mfe-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build:dev": "ng build --configuration dev --output-hashing none --output-path dist/angular-mfe-demo-dev && npm run bundle:dev",
    "build:sit": "ng build --configuration sit --output-hashing none --output-path dist/angular-mfe-demo-sit && npm run bundle:sit",
    "build:prod": "ng build --configuration prod --output-hashing none --output-path dist/angular-mfe-demo-prod && npm run bundle:prod",
    "build": "ng build",
    "bundle:dev": "webpack --config webpack.config.js --env project_name=angular-mfe-demo target_env=dev",
    "bundle:sit": "webpack --config webpack.config.js --env project_name=angular-mfe-demo target_env=sit",
    "bundle:prod": "webpack --config webpack.config.js --env project_name=angular-mfe-demo target_env=prod",
    "watch": "ng build --watch --configuration dev",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/elements": "^19.2.14",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.7",
    "@angular/cli": "^19.2.7",
    "@angular/compiler-cli": "^19.2.0",
    "@types/jasmine": "~5.1.0",
    "copy-webpack-plugin": "^13.0.0",
    "fs-extra": "^11.3.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.7.2",
    "webpack-cli": "^6.0.1"
  }
}
